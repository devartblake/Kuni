{
  "name": "KuniMixerBot",
  "productName": "Electron API Demos",
  "version": "1.3.0",
  "description": "Electron interactive API demos",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "test": "mocha && standard",
    "package": "npm-run-all package:*",
    "pack:win32": "electron-packager . Kuni --out=./dist --platform=win32 --arch=ia32 --electronVersion=1.4.15 --asar --prune --overwrite --icon=./resources/icons/logo.ico",
    "pack:win64": "electron-packager . Kuni --out=./dist --platform=win32 --arch=x64 --electronVersion=1.4.15 --asar --prune --overwrite --icon=./resources/icons/logo.ico",
    "pack:lin32": "electron-packager . Kuni --out=./dist --platform=linux --arch=ia32 --electronVersion=1.4.15 --asar --prune --overwrite --icon=./resources/icons/logo.ico",
    "pack:lin64": "electron-packager . Kuni --out=./dist --platform=linux --arch=x64 --electronVersion=1.4.15 --asar --prune --overwrite --icon=./resources/icons/logo.ico",
    "pack:all32": "electron-packager . Kuni --out=./dist --platform=all --arch=ia32 --electronVersion=1.4.15 --asar --prune --overwrite --icon=./resources/icons/logo.ico",
    "pack:all64": "electron-packager . Kuni --out=./dist --platform=all --arch=x64 --electronVersion=1.4.15 --asar --prune --overwrite --icon=./resources/icons/logo.ico",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app-icon/mac/app.icns --osx-sign.identity='Developer ID Application: GitHub' --extend-info=assets/mac/info.plist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "package:sign-exe": "signcode './out/Electron API Demos-win32-ia32/Electron API Demos.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "package:installer": "node ./script/installer.js",
    "package:sign-installer": "signcode './out/windows-installer/ElectronAPIDemosSetup.exe' --cert ~/electron-api-demos.p12 --prompt --name 'Electron API Demos' --url 'http://electron.atom.io'",
    "package:mas": "./script/mas.sh",
    "rebuild32": "node-gyp rebuild --directory=node_modules/robotjs/ --runtime=electron --target=1.4.15 --arch=ia32 --dist-url=https://atom.io/download/atom-shell",
    "rebuild64": "node-gyp rebuild --directory=node_modules/robotjs/ --runtime=electron --target=1.4.15 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "windows-store": "node ./script/windows-store.js",
    "release": "node ./script/release.js",
    "build32": "npm run pack:win32 && npm run pack:lin32",
    "build64": "npm run pack:win64 && npm run pack:lin64",
    "build-all": "echo Packaging 32bit versions && npm run rebuild32 && npm run pack:all32 && echo 32bit Packaging done.... && echo Packaging 64bit versions && npm run rebuild64 && npm run pack:all64",
    "rebuild": "npm run rebuild64",
    "build": "npm run pack:win64"
  },
  "repository": "https://github.com/electron/electron-api-demos",
  "keywords": [
    "Electron",
    "API",
    "demo"
  ],
  "author": "GitHub",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "debug": "^2.6.8",
    "devtron": "^1.3.0",
    "electron": "~1.6.2",
    "electron-debug": "^1.2.0",
    "electron-packager": "^8.6.0",
    "electron-rebuild": "^1.5.11",
    "electron-winstaller": "^2.2.0",
    "mocha": "^3.1.0",
    "npm-run-all": "^4.0.2",
    "request": "^2.81.0",
    "rimraf": "^2.5.2",
    "signcode": "^0.5.0",
    "spectron": "~3.6.0",
    "standard": "^8.2.0"
  },
  "dependencies": {
    "beam-client-node": "^0.13.0",
    "beam-interactive-node2": "^2.2.0",
    "dotenv": "^4.0.0",
    "electron-gh-releases": "^2.0.4",
    "electron-json-storage": "^2.1.1",
    "electron-oauth2": "^2.4.1",
    "electron-shortcut-normalizer": "^1.0.0",
    "glob": "^7.1.0",
    "highlight.js": "^9.3.0",
    "kbm-robot": "0.0.4",
    "node-json-db": "^0.7.3",
    "random-puppy": "^1.1.0",
    "robotjs": "^0.4.7",
    "roll": "^1.2.0",
    "underscore": "^1.8.3",
    "validator": "^7.2.0",
    "ws": "^3.0.0"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  }
}
